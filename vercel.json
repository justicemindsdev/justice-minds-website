{
  "public": true,
  "cleanUrls": false,
  "trailingSlash": false,
  "buildCommand": null,
  "outputDirectory": "public",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
